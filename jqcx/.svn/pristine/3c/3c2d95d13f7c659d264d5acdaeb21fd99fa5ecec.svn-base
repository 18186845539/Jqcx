#mongoDB连接配置
mongo.dbname=jqcx
mongo.host=cc.jqcx.net
mongo.port=27017
mongo.username=jqcxuser
mongo.password=jqcxuser
#一个线程变为可用的最大阻塞数
mongo.connectionsPerHost=8
#线程队列数,它以上面connectionsPerHost值相乘的结果就是线程队列最大值
mongo.threadsAllowedToBlockForConnectionMultiplier=40
#连接超时时间（毫秒）
mongo.connectTimeout=15000
#最大等待时间
mongo.maxWaitTime=15000
#自动重连
mongo.autoConnectRetry=true
#scoket保持活动
mongo.socketKeepAlive= true
#scoket超时时间
mongo.socketTimeout=15000
#读写分离
mongo.slaveOk=true
